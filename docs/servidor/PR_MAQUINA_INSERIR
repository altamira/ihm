SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO


ALTER PROCEDURE [dbo].[PR_MAQUINA_INSERIR] 
	@CODIGO VARCHAR(20),
	@NOME VARCHAR(50),	
	@PATH VARCHAR(50),
	@REDE VARCHAR(MAX),
	@ATUALIZACAO DATETIME2(7),
	@PARAMETROS VARCHAR(MAX),
	@ESTADO  VARCHAR(MAX)
AS
BEGIN

	INSERT INTO MAQUINA (codigo, nome, path, rede, atualizacao, parametros, estado) VALUES (@CODIGO, @NOME, @PATH, @REDE, @ATUALIZACAO, @PARAMETROS, @ESTADO)

	SELECT * FROM MAQUINA WHERE id = @@IDENTITY

END


