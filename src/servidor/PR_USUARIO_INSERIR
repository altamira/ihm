ALTER PROCEDURE [dbo].[PR_USUARIO_INSERIR] 
	@USUARIO VARCHAR(50),
	@SENHA VARCHAR(32),	
	@PERFIL VARCHAR(100),
	@EMAIL VARCHAR(50),
	@DESCRICAO VARCHAR(50),
	@ATUALIZACAO DATETIME2(7)
AS
BEGIN

	INSERT INTO USUARIO (usuario, senha, perfil, email, descricao, atualizacao) VALUES (@USUARIO, @SENHA, @PERFIL, @EMAIL, @DESCRICAO, @ATUALIZACAO)

	SELECT * FROM USUARIO WHERE id = @@IDENTITY

END


